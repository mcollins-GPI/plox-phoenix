<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <rule name="data requests" stopProcessing="true">
                    <match url="data/(.*)" />
                    <action type="Rewrite" url="http://localhost:34567/{R:1}" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>
